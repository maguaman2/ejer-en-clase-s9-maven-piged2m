<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="371.0" prefWidth="499.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.empresa.PrimaryController">
   <children>
      <Label text="Fruta" />
      <TextField fx:id="txtCode" />
      <TextField fx:id="txtName" />
      <Button mnemonicParsing="false" onAction="#botonAgregar" text="Agregar" />
      <Button mnemonicParsing="false" onAction="#imprimirColeccion" text="imprimir" />
      <Label text="Primera Pantalla" />
      <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Ingresar" />
      <Button mnemonicParsing="false" onAction="#archivos" text="Archivos" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
